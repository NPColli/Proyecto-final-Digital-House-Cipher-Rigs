      <label class="labelForm">Contraseña</label>
      <input type="password" class="control" name="apellido" placeholder="Ingrese su Contraseña">
      <br>
      <button type="submit" class="buttons" href="/views/register.html">Siguiente</button>
      <br><br>
      <p><a class="pass" href="#">¿Olvidó su contraseña?</a></p>
      <br>
      <button class="buttons" action='register' type="submit"><a class="buttonCuenta" href="register">Crear Cuenta</a></button>

    </form>
  </main>
  <footer>
    <%- include('../partials/footer') %>
  </footer>
</body>

</html>